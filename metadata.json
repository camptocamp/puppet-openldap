{
  "name": "camptocamp-openldap",
  "version": "1.2.0",
  "author": "Camptocamp",
  "summary": "Puppet OpenLDAP module",
  "license": "Apache-2.0",
  "source": "https://github.com/camptocamp/puppet-openldap",
  "project_page": "https://github.com/camptocamp/puppet-openldap",
  "issues_url": "https://github.com/camptocamp/puppet-openldap/issues",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=3.2.0 <5.0.0"
    },
    {
      "name": "herculesteam/augeasproviders_core",
      "version_requirement": "2.x"
    },
    {
      "name": "herculesteam/augeasproviders_shellvar",
      "version_requirement": "2.x"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": "3.x"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "10.04",
        "12.04",
        "14.04"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6"
      ]
    }
  ],
  "puppet_version": [
    "3.7"
  ]
}
